<?xml version="1.0"?>
<robot name="peg">

  <link name="base">
    <visual>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
      <material name="box">
        <color rgba="1. 0. 0. 1"/>
      </material>
    </visual>
  </link>

  <link name="dummy">
    <visual>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
      <material name="box">
        <color rgba="0. 1. 0. 1"/>
      </material>
    </visual>
  </link>

  <link name="dummy2">
    <visual>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
      <material name="dummy2">
        <color rgba="0. 1. 0. 1"/>
      </material>
    </visual>
  </link>


  <joint name="p_joint_x" type="prismatic">
    <axis xyz="1 0 0"/>
    <origin xyz="0 0 0"/>
    <parent link="base"/>
    <child link="dummy"/>
    <limit effort="1.0" lower="-1.0" upper="1.0" velocity="5.0"/>
  </joint>

  <joint name="p_joint_y" type="prismatic">
    <axis xyz="0 1 0"/>
    <origin xyz="0 0 0"/>
    <parent link="dummy"/>
    <child link="dummy2"/>
    <limit effort="1.0" lower="-1.0" upper="1.0" velocity="5.0"/>
  </joint>

  <link name="ee">
    <visual>
      <geometry>
        <box size="0.25 0.25 0.2"/>
      </geometry>
      <material name="box">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.25 0.25 0.2"/>
      </geometry>
      <contact_coefficients mu=".00005" kp="0." kd="0."/>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <mass value="1."/>
      <inertia ixx="0.00541667" ixy="0" ixz="0" iyy="0.02104167" iyz="0" izz="0.02604167"/>
    </inertial>
  </link>

  <joint name="r_joint_z" type="revolute">
    <axis xyz="0 0 1"/>
    <origin xyz="0 0 0.0"/>
    <parent link="dummy2"/>
    <child link="ee"/>
    <limit effort="1.0" lower="-31.4159265359" upper="31.4159265359" velocity="5.0"/>
  </joint>

</robot>

